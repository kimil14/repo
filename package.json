{
  "name": "launcher-warfull",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "minecraft-launcher-core": "^3.18.1"
  },
  "build": {
    "appId": "fr.kimil.launcher.warfull",
    "productName": "LauncherMinecraftWarfull",
    "files": [
      "build/**/*",
	  "assets/**/*",
      "node_modules/**/*",
      "main.js",
      "package.json",
      "launcher2.js",
      "preload.js",
      "index.html"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icon.png"
    }
  },
  "devDependencies": {
    "electron": "^32.2.0",
    "electron-builder": "^25.1.8"
  },
  "description": ""
}
